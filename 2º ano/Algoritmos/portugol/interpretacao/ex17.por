programa {
  funcao inicio() {
    //Inicialização
    inteiro contador= 1, n1
    escreva("Digite um valor: ")
    leia(n1)
    //Condição
    enquanto(contador<= n1) {
    

    //Ação
    escreva(contador, "\n")

