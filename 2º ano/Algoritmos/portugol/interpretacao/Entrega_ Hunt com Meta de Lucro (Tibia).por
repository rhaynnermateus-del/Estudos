programa {
  funcao inicio() {

    inteiro total_gp=1 , gp

    enquanto(total_gp<100000){

  escreva("Hunt com Meta de Lucro")

    escreva("Meta:100000 GP")

    escreva("GP atual: ")

    escreva("Digite o valor do próximo item: ")

    leia(gp)


  total_gp=total_gp+gp 

    }
escreva("---Relatório Final da Hunt---")
  escreva("Meta alcançada")
  escreva("Total de GP coletado:", total_gp, "\n")

    }
    
  }
}
