programa {
  cadeia nome
  inteiro n1,n2,media = 7.0
  funcao inicio() {
    escreva("Qual é a sua primeira nota?")
    leia(n1)
    escreva("Qual é a sua segunda nota?")
    leia(n2)

    n1 + n2 = media 

    se (n1 e n2 < media)
    escreva("Você não possui a média")

    se(n1 e n2> media)
    escreva("você conseguiu a média")



    
  }
}
