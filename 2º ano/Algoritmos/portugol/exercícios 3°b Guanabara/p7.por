programa {
  real numero, multiplicacao,divisao

  funcao inicio() {
    escreva("Digite um número: ")
    leia(numero)
    multiplicacao=numero*2
    escreva("O dobro de ", numero, " é ", multiplicacao,"\n")
    divisao=numero/1/3
    escreva("A terça parte de ", numero, " é ", divisao)

    
  }
}
